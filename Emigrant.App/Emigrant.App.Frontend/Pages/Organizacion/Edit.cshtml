@page
@model Emigrant.App.Frontend.Pages.EditOrganizacionModel
@{
    var org = Model.Organizacion;
}
 
<div class="text-center">
<form method="POST">
  <label for="id" class="form-label">id:</label>
  <input type="number" class="form-control" id="id" name="id" placeholder="id" asp-for="@org.id">
  <label for="Razon Social" class="form-label">Razon Social:</label>
  <input type="text" class="form-control" id="razonsocial" name="razonsocial" placeholder="Razon Social" asp-for="@org.razonsocial">
  <label for="Nit" class="form-label">Nit:</label>
  <input type="text" class="form-control" id="nit" name="nit" placeholder="Nit" asp-for="@org.nit">
  <label for="Direccion" class="form-label">Direccion:</label>
  <input type="text" class="form-control" id="direccion" name="direccion" placeholder="Direccion" asp-for="@org.direccion">
  <label for="Ciudad" class="form-label">Ciudad:</label>
  <input type="text" class="form-control" id="ciudad" name="ciudad" placeholder="Ciudad" asp-for="@org.ciudad">
  <label for="Telefono" class="form-label">Telefono:</label>
  <input type="text" class="form-control" id="telefono" name="telefono" placeholder="Telefono" asp-for="@org.telefono">
  <label for="Direccion electronica" class="form-label">Direccion electronica:</label>
  <input type="text" class="form-control" id="correo" name="correo" placeholder="Direccion electronica" asp-for="@org.correo">
  <label for="paginaweb" class="form-label">pagina web:</label>
  <input type="text" class="form-control" id="paginaweb" name="paginaweb" placeholder="Pagina Web" asp-for="@org.paginaweb">
  <label for="sector" class="form-label">Secto:</label>
  <input type="text" class="form-control" id="sector" name="sector" placeholder="Sector" asp-for="@org.sector"> 
  <label for="servicio" class="form-label">Servicio:</label>
  <input type="text" class="form-control" id="servicioofrece" name="servicioofrece" placeholder="Servicio Ofrece" asp-for="@org.servicioofrece">
  <hr>
  <button type="submit" class="btn btn-success">Enviar</button>
</form>
</div>